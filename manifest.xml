<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="MailApp">
  <Id>7140fa3e-5622-4657-9ca1-ab198ce87b67</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Joe Martella</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Smarter Email"/>
  <Description DefaultValue="Makes your simple emails sound smarter."/>
  <HighResolutionIconUrl DefaultValue="https://raw.githubusercontent.com/martellaj/smarter-email/master/app-icon.png"/>
  <!--<SupportUrl DefaultValue="[Support URL]"/>-->
  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>
  <Requirements>
    <Sets>
      <Set Name="MailBox" MinVersion="1.1"/>
    </Sets>
  </Requirements>
  <FormSettings>
    <Form xsi:type="ItemEdit">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://localhost:8443/appcompose/index.html"/>
      </DesktopSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteItem</Permissions>
  <Rule xsi:type="ItemIs" ItemType="Message" FormType="Edit"/>
  <DisableEntityHighlighting>false</DisableEntityHighlighting>
</OfficeApp>